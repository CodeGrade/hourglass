<div id="q<%= question_number + 1 %>p<%= part_number + 1 %>" class="row part">
  <div class="col-12">
    <h3>Part <%= part_number + 1 %>: <%= part["name"] %></h3>
    <h4>This part is worth <%= part["points"] %> points.</h4>
    <p><%= part["description"]&.html_safe %></p>
    <% if part["files"] %>
      <%= render "filepicker", files: @exam.get_exam_files(part["files"]), root: "question_#{question_number}_part_#{part_number}" %>
    <% end %>
    <% part["body"].each_with_index do |item, index| %>
      <%= render "bodyitem", form: form, item: item, question_number: question_number, part_number: part_number, bodyitem_number: index %>
    <% end %>
  </div>
</div>