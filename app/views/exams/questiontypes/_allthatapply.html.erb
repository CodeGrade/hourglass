<%= render "exams/prompt", item: item %>
<%= @form.form_group @current, label: { text: "Select all that apply." } do %>
  <% num = 0 # TODO: are these always in order? %>
  <% item["options"].each do |option| %>
    <%= @form.check_box "#{@current}_#{num}", label: option.keys[0] %>
    <% num += 1 %>
  <% end %>
<% end %>
