{
  "name": "hourglass",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "private": true,
  "scripts": {
    "webpack-dev": "./bin/webpack-dev-server",
    "lint": "eslint 'app/packs' --ext 'js,ts,jsx,tsx'",
    "typecheck": "tsc --noEmit --noErrorTruncation --pretty false --incremental false -p .",
    "uml": "tplant -C --input app/packs/components/workflows/student/exams/show/types.ts --output types.svg",
    "relay-dev": "relay-compiler --watch",
    "relay": "relay-compiler --watchman false",
    "relay-persist": "relay-compiler --watchman false --persist-output ./config/schemas/graphql-queries.json",
    "dev": "concurrently \"yarn run webpack-dev\" \"yarn run relay-dev\"",
    "unused": "ts-prune"
  },
